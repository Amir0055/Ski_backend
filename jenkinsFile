pipeline{
    agent any
    stages {
        stage("Testing Maven"){
            steps {
                sh "mvn -version"
            }
        }
        stage("Testing JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
            }
        }
        stage('MVN COMPILE') {
            steps{
                sh 'mvn compile'
        }
}
    stage('MVN SONARQUBE') {
            steps{

                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar'
            }
        }
      stage('MVN Nexus') {
            steps{
                sh 'mvn deploy -Dmaven.test.skip=true'
        }
}
    stage('Docker Image ') {
            steps{
                sh 'docker build -t khouloudaskri/gestion-station-ski-1.0 .'
        }
}
    stage('Docker HUB') {
            steps{
                sh 'docker login -u khouloudaskri -p Docker2023'
                sh 'docker push khouloudaskri/gestion-station-ski-1.0'
        }
}
    }
}